{
  "configurations": {
    "<name>: Launch": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "name": "gdb",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceRoot}/target/debug/${workspaceFolderBasename}",
        "stopAtEntry": true,
        "args": [],
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb"
      }
    }
  }
}
